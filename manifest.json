{
    "name": "Gmail Inbox Menu Slide Out",
    "version": "0.1",
    "icons": { 
        "16": "/img/icons/icon_16.png",
        "48": "/img/icons/icon_48.png",
        "128": "/img/icons/icon_128.png" 
    },

    "description": "Slides out the inbox.google.com menu on load and keeps it open.",

    "content_scripts": [
        {
            "matches": ["https://inbox.google.com/*"],
            "js": [
                    "/js/external/jquery.js", 
                    "/js/content_script.js"
                ]
        }
    ],

    "manifest_version": 2
}
